.model small
.STACK 100
.data
ARRAY DB 12H,23H,26H,63H,25H,86H,2FH,33H,10H,35H
ARR_ODD DB 10 DUP(?)
ARR_Even DB 10 DUP(?)
.code
START:MOV AX,@data
MOV DS,AX
MOV CL,10
XOR DI,DI
XOR SI,SI
LEA BP,ARRAY
BACK:MOV AL,DS:[BP]
AND AL,01H
JZ NEXT
LEA BX,ARR_ODD
MOV AH,[BX+DI]
MOV ARR_ODD,AH
INC DI
JMP SKIP
NEXT : LEA BX,ARR_EVEN
MOV AH,[BX+SI]
MOV AH,ARR_EVEN 
INC SI
INC BP
DEC CL
JNZ BACK
END START
